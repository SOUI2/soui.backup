<?xml version="1.0" encoding="utf-8"?>
<config>
  <filetypes>
    <filetype type="C源文件" ext="c" singleLineRemark="//" multiLinesRemarkBegin="/*" multiLinesRemarkEnd="*/"/>
    <filetype type="C/C++头文件" ext="h" singleLineRemark="//" multiLinesRemarkBegin="/*" multiLinesRemarkEnd="*/"/>
    <filetype type="模板头文件" ext="hpp" singleLineRemark="//" multiLinesRemarkBegin="/*" multiLinesRemarkEnd="*/"/>
    <filetype type="c++头文件" ext="hh" singleLineRemark="//" multiLinesRemarkBegin="/*" multiLinesRemarkEnd="*/"/>
    <filetype type="C++源文件" ext="cpp" singleLineRemark="//" multiLinesRemarkBegin="/*" multiLinesRemarkEnd="*/"/>
    <filetype type="CC源文件" ext="cc" singleLineRemark="//" multiLinesRemarkBegin="/*" multiLinesRemarkEnd="*/"/>
    <filetype type="资源文件" ext="rc" singleLineRemark="//" multiLinesRemarkBegin="/*" multiLinesRemarkEnd="*/"/>
    <filetype type="java源文件" ext="java" singleLineRemark="//" multiLinesRemarkBegin="/*" multiLinesRemarkEnd="*/"/>

    <filetype type="XML文件" ext="xml" singleLineRemark="" multiLinesRemarkBegin="&lt;!--" multiLinesRemarkEnd="--&gt;"/>

    <filetype type="LUA文件" ext="lua" singleLineRemark="--" multiLinesRemarkBegin="--[[" multiLinesRemarkEnd="]]"/>
  </filetypes>

  <languages>
    <language name="C/C++" exts="c;h;hh;hpp;cpp;cc"/>
    <language name="XML" exts="xml"/>
    <language name="LUA" exts="lua"/>
    <language name="java" exts="java" />

  </languages>
</config>
