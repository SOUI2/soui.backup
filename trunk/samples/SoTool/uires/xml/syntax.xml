<?xml version="1.0" encoding="utf-8"?>
<config>
  <filetypes>
    <filetype type="C源文件" ext="c" singleLineRemark="//" multiLinesRemarkBegin="/*" multiLinesRemarkEnd="*/"/>
    <filetype type="C/C++头文件" ext="h" singleLineRemark="//" multiLinesRemarkBegin="/*" multiLinesRemarkEnd="*/"/>
    <filetype type="模板头文件" ext="hpp" singleLineRemark="//" multiLinesRemarkBegin="/*" multiLinesRemarkEnd="*/"/>
